ul(id="places")
    if lugares
        each item in lugares
            li
                a(href="#",title=item.titulo,data-latlng=item.latLng,id=item.id)
                    img(src=item.imagen,width="100",height="100",alt=item.titulo)
                    div
                        div(class="center")
                            h3= item.descripcion
                            p= item.titulo